<div class = "modal-header">
       <% var oneItem = rows[0] %>
       <%=oneItem.send_name%> 님이 보낸 쪽지
       보낸 시간 : <%=oneItem.date_sent%>
      <button type = "button" class = "close" data-dismiss="modal" aria-label = "Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div> <!-- modal-header -->

    <div class= "modal-body">
        <textarea row= "10" cols= "50" readonly>
          <%oneItem.content%>
        </textarea>
    </div> <!-- modal-body -->
    <div class = "modal-footer">
        <button type = "button" class = "btn btn-default" data-dismiss = "modal" id="Replybtn"> 답장 </button>
      <button type = "button" class = "btn btn-default" data-dismiss = "modal" id="Okbtn"> 확인</button>
  </div>
